{
  "description": "chain function definition",
  "type": "object",
  "required": ["function", "variable", "functions"],
  "additionalProperties": false,
  "properties": {
    "function": { "type": "string", "const": "chain" },
    "variable": { "type": "string" },
    "functions": {
      "type": "array",
      "minItems": 1,
      "items": {
        "oneOf": [
          { "$ref": "./base.json" },
          { "$ref": "./first_non_empty.json" },
          { "$ref": "./format.json" },
          { "$ref": "./get.json" },
          { "$ref": "./join.json" },
          { "$ref": "./map_string.json" },
          { "$ref": "./postfixed_street.json" },
          { "$ref": "./regexp.json" },
          { "$ref": "./remove_postfix.json" },
          { "$ref": "./remove_prefix.json" }
        ]
      }
    }
  }
}
