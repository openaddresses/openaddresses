{
    "coverage": {
        "ISO 3166": {
            "alpha2": "BR-PI",
            "country": "Brazil",
            "state": "Piauí"
        },
        "country": "br",
        "state": "pi"
    },
    "website": "http://www.censo2010.ibge.gov.br/cnefe/",
