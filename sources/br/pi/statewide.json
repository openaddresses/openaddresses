    "protocol": "http",
    "compression": "zip",
    "conform": {
        "format": "csv",
        "lat": "lat",
        "lon": "lon",
        "number": [
            "number",
            "number suffix"
        ],
        "street": [
            "street type",
            "street title",
            "street name"
        ],
        "unit": [
            "unit k1", "unit v1",
            "unit k2", "unit v2",
            "unit k3", "unit v3",
            "unit k4", "unit v4",
