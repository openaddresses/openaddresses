        "city": "municipality",
        "region": "state",
        "postcode": {
            "function": "regexp",
            "field": "postal code",
            "pattern": "^([0-9]{5})([0-9]{3})$",
            "replace": "$1-$2"
        },
        "accuracy": 5
    }
