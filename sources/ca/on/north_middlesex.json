{
    "coverage": {
        "country": "ca",
        "state": "on",
        "county": "North Middlesex"
    },
    "schema": 2,
    "layers": {
        "addresses": [
            {
                "name": "county",
                "website": "https://www.middlesex.ca/departments/mapping#interactive_mapping",
                "data": "https://maps.middlesex.ca/arcgis/rest/services/Public/NMID_Address/MapServer/0",
                "protocol": "ESRI",
                "conform": {
                    "format": "geojson",
                    "number": {
                        "function": "regexp",
                        "field": "FULLADDRES",
                        "pattern": "^([0-9]+)"
                    },
                    "street": {
                        "function": "regexp",
                        "field": "FULLADDRES",
                        "pattern": "^(?:[0-9]+ )(.*)",
                        "replace": "$1"
                    }
                }
            }
        ]
    }
}