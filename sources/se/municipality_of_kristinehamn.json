{
    "coverage": {
        "country": "se",
        "city": "Kristinehamn"
    },
    "data": "http://kartor-va.kristinehamn.se/arcgis/rest/services/Bakgrundskartor/Bakgrundskarta_fargad_enkel/MapServer/112",
    "type": "ESRI",
    "conform": {
        "type": "geojson",
        "number": {
            "function": "regexp",
            "field": "Name",
            "pattern": "([0-9]+[A-Z])$"
        },
        "street": {
            "function": "regexp",
            "field": "Name",
            "pattern": "^(.*?)( [0-9]+[A-Z])?$"
        },
        "city": "PostCity",
        "postcode": {
            "function": "regexp",
            "field": "PostCode",
            "pattern": "^([\\d]{3})([\\d]{2})$",
            "replace": "$1 $2"
        }
    }
}
