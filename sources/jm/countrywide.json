            "field": "LOCATION",
            "pattern": "^(?:C\\s*/\\s*O\\s*)?(.*?)\\s*(?:(?:P(?:\\.|\\s*)\\s*(?:[OA0]\\.?|A\\.O\\.?|OP|O O)(?:\\s+(?:NO\\.?|#))?\\s*[\\d]*[\\s\\.]*)|(?:\\s+P\\s+OP?\\s+.*)|(?:\\s*(?:NO\\.?|#)\\s*[\\d]*\\s*)|(?:\\sP\\s*))?$"
        },
        "region": "PARISH"
    }
}
