{
    "coverage": {
        "country": "jm",
        "ISO 3166": {
            "alpha2": "jm",
            "country": "Jamaica"
        }
    },
    "protocol": "http",
    "compression": "zip",
    "data": "https://data.openaddresses.io/cache/uploads/trescube/936e31/jamaica.geojson.zip",
    "conform": {
        "format": "geojson",
        "number": "STREET_",
