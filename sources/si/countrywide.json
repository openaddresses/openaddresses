{
    "website": "http://egp.gu.gov.si/egp/",
    "data": "http://raba.openstreetmap.si/openaddresses/si-addresses-latest.zip",
    "note": "Updated periodically. Source must be regenerated using script in this repo's /scripts/si directory.",
    "type": "http",
    "compression": "zip",
    "coverage": {
        "ISO 3166": {
            "alpha2": "SI",
            "country": "Slovenia"
        },
        "country": "si"
    },
    "license": {
        "text": "CC BY 2.5 SI",
        "url": "https://creativecommons.org/licenses/by/2.5/si/legalcode",
        "attribution": true,
        "attribution name": "Geodetska uprava Republike Slovenije / The Surveying and Mapping Authority of the Republic of Slovenia, Register prostorskih enot, 2017",
        "share-alike": false
    },
    "conform": {
        "encoding": "UTF-8",
        "srs": "EPSG:4326",
        "type": "csv",
        "csvsplit": ";",
        "accuracy": 1,
        "lon": "lon",
        "lat": "lat",
        "number": "number",
        "street": "street",
        "city": "city",
        "district": "commune",
        "region": "region",
        "postcode": "postcode",
        "id": "id"
    }
}
