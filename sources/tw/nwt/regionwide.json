{
    "coverage": {
        "ISO 3166": {
            "alpha2": "TW-NWT",
            "country": "Taiwan"
        },
        "country": "tw"
    },
    "data": "http://rain.ntpc.gov.tw/ArcGIS/rest/services/Sewer/Basemap/MapServer/1",
    "type": "ESRI",
    "conform": {
        "type": "geojson",
        "number":{
            "function": "regexp",
            "field": "NO_CH",
            "pattern": "^([0-9\uff10-\uff19]+\u865f?)"
        },
        "unit":{
            "function": "regexp",
            "field": "NO_CH",
            "pattern": "^(?:[0-9\uff10-\uff19]+\u865f?)(.*)"
        },
        "street": {
            "function": "join",
            "fields": ["ROADSEC","LANE_C","LONG_C"],
            "separator": ""
        },
        "city": "TOWN_NAME"
    }
}
