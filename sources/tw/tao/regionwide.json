{
    "coverage": {
        "ISO 3166": {
            "alpha2": "TW-TAO",
            "country": "Taiwan"
        },
        "country": "tw"
    },
    "data": "http://gener.tycg.gov.tw/arcgis/rest/services/map/MapServer/18",
    "type": "ESRI",
    "conform": {
        "type": "geojson",
        "number":{
            "function": "regexp",
            "field": "HOUSE_NO",
            "pattern": "^([0-9\uff10-\uff19]+\u865f?)"
        },
        "unit":{
            "function": "regexp",
            "field": "HOUSE_NO",
            "pattern": "^(?:[0-9\uff10-\uff19]+\u865f?)(.*)"
        },
        "street": {
            "function": "join",
            "fields": ["STREET","SECT","LANE","ALLEY"],
            "separator": ""
        },
        "city": "TOWNNAME",
        "district": "LI",
        "region": "COUNTY"
    }
}
