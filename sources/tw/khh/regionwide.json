{
    "coverage": {
        "ISO 3166": {
            "alpha2": "TW-KHH",
            "country": "Taiwan"
        },
        "country": "tw"
    },
    "schema": 2,
    "layers": {
        "addresses": [
            {
                "name": "country",
                "data": "http://dengue.kcg.gov.tw/arcgis/rest/services/KCG/KcgAddress/MapServer/0",
                "protocol": "ESRI",
                "conform": {
                    "format": "geojson",
                    "number": {
                        "function": "regexp",
                        "field": "NO1",
                        "pattern": "^([0-9０-９]+號?)"
                    },
                    "unit": {
                        "function": "regexp",
                        "field": "NO1",
                        "pattern": "^(?:[0-9０-９]+號?)(.*)"
                    },
                    "street": {
                        "function": "join",
                        "fields": [
                            "ROADNAME",
                            "SECT",
                            "LAN",
                            "LON"
                        ],
                        "separator": ""
                    },
                    "city": "VILLAGE",
                    "district": "TOWN",
                    "region": "COUNTY"
                }
            }
        ]
    }
}