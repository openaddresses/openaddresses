{
    "coverage": {
        "ISO 3166": {
            "alpha2": "TW-ILA",
            "country": "Taiwan"
        },
        "country": "tw"
    },
    "data": "http://ja.miaoli.gov.tw/arcgis/rest/services/Miaoli/miaoliDpDynamic_B/FeatureServer/1",
    "type": "ESRI",
    "language": "zh",
    "conform": {
        "type": "geojson",
        "number":{
            "function": "regexp",
            "field": "NUM",
            "pattern": "^([0-9\uff10-\uff19]+\u865f?)"
        },
        "unit":{
            "function": "regexp",
            "field": "NUM",
            "pattern": "^(?:[0-9\uff10-\uff19]+\u865f?)(.*)"
        },
        "street": {
            "function": "join",
            "fields": ["ROAD","LANE","ALLEY"],
            "separator": ""
        },
        "city": "VILLAGE",
        "district": "TOWN"
    }
}
