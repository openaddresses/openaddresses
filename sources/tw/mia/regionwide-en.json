    "type": "ESRI",
    "language": "en",
    "conform": {
        "type": "geojson",
        "number":{
            "function": "regexp",
            "field": "ENUM",
            "pattern": "^([0-9\uff10-\uff19]+)"
        },
        "unit":{
            "function": "regexp",
            "field": "ENUM",
            "pattern": "^(?:[0-9\uff10-\uff19]+-?)(.*)"
        },
        "street": {
            "function": "join",
            "fields": ["EROAD","ELANE","EALLEY"],
            "separator": ""
        },
        "city": "EVILLAGE",
        "district": "ETOWN"
    }
}
