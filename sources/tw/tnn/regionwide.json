{
    "coverage": {
        "ISO 3166": {
            "alpha2": "TW-TNN",
            "country": "Taiwan"
        },
        "country": "tw"
    },
    "data": "http://water.tainan.gov.tw/tnwrbarcgis/rest/services/TNRW/Address/MapServer/0",
    "protocol": "ESRI",
    "conform": {
        "format": "geojson",
        "number":{
            "function": "regexp",
            "field": "NO",
            "pattern": "^([0-9\uff10-\uff19]+\u865f?)"
        },
        "unit":{
            "function": "regexp",
            "field": "NO",
            "pattern": "^(?:[0-9\uff10-\uff19]+\u865f?)(.*)"
        },
        "street": {
            "function": "join",
            "fields": ["ROADNAME","LANE"],
            "separator": ""
        },
        "city": "VILLAGE",
        "district": "DISTRICT",
        "region": "COUNTY"
    }
}
