{
    "coverage": {
        "US Census": {
            "geoid": "20041",
            "name": "Dickinson County",
            "state": "Kansas"
        },
        "country": "us",
        "state": "ks",
        "county": "Dickinson"
    },
    "conform": {
        "type": "geojson",
        "number": {
            "function": "prefixed_number",
            "field": "PropertyAddress"
        },
        "street": {
            "function": "regexp",
            "field": "PropertyAddress",
            "pattern": "^([0-9]+ )(.*),(.*),(.*)([0-9]{5})$",
            "replace": "$2"
        },
        "city": {
            "function": "regexp",
            "field": "PropertyAddress",
            "pattern": "^(?:[0-9]+ )(.*),(.*),(.*)([0-9]{5})$",
            "replace": "$3"
        },
         "postcode": {
            "function": "regexp",
            "field": "PropertyAddress",
            "pattern": ".*([0-9]{5})$",
            "replace": "$1"
        }
    },
    "data": "http://dkcoksgis.org/arcgis/rest/services/PublicMaps/ParcelSearch/MapServer/28",
    "type": "ESRI"
}
