{
    "coverage": {
        "US Census": {
            "geoid": "04023",
            "name": "Santa Cruz County",
            "state": "Arizona"
        },
        "country": "us",
        "state": "az",
        "county": "Santa Cruz"
    },
    "data": "https://gis.santacruzcountyaz.gov/arcgis/rest/services/ParcelSearch/Parcels/MapServer/0",
    "protocol": "ESRI",
    "note": "SITEADDR has the city after the road name, so the split is going to be crummy",
    "conform": {
        "format": "geojson",
        "number": {
            "function": "regexp",
            "field": "SITEADDR",
            "pattern": "^([0-9]+)"
        },
        "street": {
            "function": "regexp",
            "field": "SITEADDR",
            "pattern": "^(?:[0-9]+ )(.*)",
            "replace": "$1"
        }
    }
}
