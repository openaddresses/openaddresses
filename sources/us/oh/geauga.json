{
    "coverage": {
        "US Census": {
            "geoid": "39055",
            "name": "Geauga County",
            "state": "Ohio"
        },
        "country": "us",
        "state": "oh",
        "county": "Geauga"
    },
    "schema": 2,
    "layers": {
        "addresses": [
            {
                "name": "county",
