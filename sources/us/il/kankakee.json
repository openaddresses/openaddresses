{
    "coverage": {
        "US Census": {
            "geoid": "17091",
            "name": "Kankakee County",
            "state": "Illinois"
        },
        "country": "us",
        "state": "il",
        "county": "Kankakee"
    },
    "schema": 2,
    "layers": {
        "addresses": [
            {
                "name": "county",
                "website": "http://www.k3gis.com/",
                "license": "http://www.k3gis.com/#contact",
                "compression": "zip",
                "data": "http://webfiles.k3gis.com/download/cadastral/Parcels.zip",
                "protocol": "http",
                "conform": {
                    "format": "shapefile-polygon",
                    "number": {
                        "function": "regexp",
                        "field": "site_addre",
                        "pattern": "^([0-9]+)"
                    },
                    "street": {
                        "function": "regexp",
                        "field": "site_addre",
                        "pattern": "^(?:[0-9]+ )(.*)",
                        "replace": "$1"
                    },
                    "city": {
                        "function": "regexp",
                        "field": "site_csz",
                        "pattern": "^(.*?),(?:.*)"
                    },
                    "postcode": {
                        "function": "regexp",
                        "field": "site_csz",
                        "pattern": "^(?:.*?)([0-9]+(?:\\-[0-9]+)?)",
                        "replace": "$1"
                    },
                    "id": "pin",
                    "accuracy": 2
                }
            }
        ]
    }
}
