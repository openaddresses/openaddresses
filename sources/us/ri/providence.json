                    },
                    "unit": {
                        "function": "regexp",
                        "field": "property_a",
                        "pattern": "^(?:\\d+(?:\\w+|-\\d+|/2)?)\\s+(?:.+?)(\\b(?:Unit|Bldg|Apt)\\b(?:\\s+\\w+)?)$"
