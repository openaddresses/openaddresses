{
    "coverage": {
        "geometry": {
            "type": "Point",
            "coordinates": [ -96.6134262084961, 33.196609461052844 ]
        },
        "US Census": {
            "geoid": "4845744",
            "name": "City of McKinney",
            "state": "Texas"
        },
        "country": "us",
        "state": "tx",
        "county": "Taylor",
        "city": "McKinney"
    },
    "data": "http://maps.mckinneytexas.org/mckinney/rest/services/OpenData/Property_Ownership/MapServer/0",
    "type": "ESRI",
    "conform": {
        "type": "geojson",
        "number": {
            "function": "prefixed_number",
            "field": "SitusAdd"
        },
        "street": {
            "function": "regexp",
            "field": "SitusAdd",
            "pattern": "^\\d+\\s(.+?)(?:\\s(?:BLDG|STE)\\s+\\w+\\s*)?$"
        },
        "unit": {
            "function": "regexp",
            "field": "SitusAdd",
            "pattern": "^\\d+\\s.+?(\\s(?:BLDG|STE)\\s+\\w+\\s*)$"
        },
        "city": "CITY"
    },
    "test": {
        "enabled": true,
        "description": "tests that verify that units are being extracted",
        "acceptance-tests": [
            {
                "description": "address without unit",
                "inputs": {
                    "SitusAdd": "1003 W LAMAR ST"
                },
                "expected": {
                    "number": "1003",
                    "street": "W LAMAR ST",
                    "unit": ""
                }
            },
            {
                "description": "address with BLDG numeric-only unit",
                "inputs": {
                    "SitusAdd": "900 S TENNESSEE ST BLDG 1"
                },
                "expected": {
                    "number": "900",
                    "street": "S TENNESSEE ST",
                    "unit": "BLDG 1"
                }
            },
            {
                "description": "address with BLDG alpha-numeric unit",
                "inputs": {
                    "SitusAdd": "900 S TENNESSEE ST BLDG 11A"
                },
                "expected": {
                    "number": "900",
                    "street": "S TENNESSEE ST",
                    "unit": "BLDG 11A"
                }
            },
            {
                "description": "address with STE numeric-only unit",
                "inputs": {
                    "SitusAdd": "5610 N MCDONALD ST STE C"
                },
                "expected": {
                    "number": "5610",
                    "street": "N MCDONALD ST",
                    "unit": "STE C"
                }
            }
        ]
    }
}
