{
    "coverage": {
        "US Census": {
            "geoid": "48339",
            "name": "Montgomery County",
            "state": "Texas"
        },
        "country": "us",
        "state": "tx",
        "county": "Montgomery"
    },
    "data": "http://services1.arcgis.com/PRoAPGnMSUqvTrzq/ArcGIS/rest/services/Tax_Parcel_Public_View/FeatureServer/0",
    "type": "ESRI",
    "conform": {
        "type": "geojson",
        "number": {
            "function": "regexp",
            "field": "PropertyAd",
            "pattern": "^(\\d+(?:\\s+1/2)?)(?:-\\d+|-?(?:[A-Z]/)*[A-Z])?\\b"
        },
        "street": {
            "function": "regexp",
            "field": "PropertyAd",
            "pattern": "^(?:\\d+(?:\\s+1/2)?(?:-\\d+|-?(?:[A-Z]/)*[A-Z])?)?\\b(.+?)\\s*,"
        },
        "unit": {
          "function": "regexp",
          "field": "PropertyAd",
          "pattern": "^(?:\\d+(?:\\s+1/2)?)(-\\d+|-?(?:[A-Z]/)*[A-Z])\\b"
        },
        "city": {
            "function": "regexp",
            "field": "PropertyAd",
            "pattern": ",\\s*(.+?)\\s*,\\s+(?:TX)\\s+"
        },
        "region": {
            "function": "regexp",
            "field": "PropertyAd",
            "pattern": "\\b(TX)\\b"
        },
        "postcode": {
            "function": "regexp",
            "field": "PropertyAd",
            "pattern": "\\s(\\d{5})$"
        }
    },
    "test": {
        "enabled": true,
        "description": "full addresses with city, state, and zip are in a single field",
        "acceptance-tests": [
            {
                "description": "empty row",
                "inputs": {
                    "PropertyAd": ""
                },
                "expected": {
                    "number": "",
                    "street": "",
                    "unit": "",
                    "city": "",
                    "region": "",
                    "postcode": ""
                }
            },
            {
                "description": "only-numbers house number",
                "inputs": {
                    "PropertyAd": "66 MCCLELLAN CIR, HUMBLE, TX  77339"
                },
                "expected": {
                    "number": "66",
                    "street": "MCCLELLAN CIR",
                    "unit": "",
                    "city": "HUMBLE",
                    "region": "TX",
                    "postcode": "77339"
                }
            },
            {
                "description": "number is hyphenated",
                "inputs": {
                    "PropertyAd": "23207-2 DECKER PRAIRIE ROSEHILL RD, MAGNOLIA, TX  77355"
                },
                "expected": {
                    "number": "23207",
                    "street": "DECKER PRAIRIE ROSEHILL RD",
                    "unit": "-2",
                    "city": "MAGNOLIA",
                    "region": "TX",
                    "postcode": "77355"
                }
            },
            {
                "description": "number is hyphenated w/alpha",
                "inputs": {
                    "PropertyAd": "20111-A PLANTATION CREEK DR, PORTER, TX  77365"
                },
                "expected": {
                    "number": "20111",
                    "street": "PLANTATION CREEK DR",
                    "unit": "-A",
                    "city": "PORTER",
                    "region": "TX",
                    "postcode": "77365"
                }
            },
            {
                "description": "number is hyphenated w/2 /-delimited alphas",
                "inputs": {
                    "PropertyAd": "37019-A/B HICKORY RIDGE RD, MAGNOLIA, TX  77354"
                },
                "expected": {
                    "number": "37019",
                    "street": "HICKORY RIDGE RD",
                    "unit": "-A/B",
                    "city": "MAGNOLIA",
                    "region": "TX",
                    "postcode": "77354"
                }
            },
            {
                "description": "number is hyphenated w/3 /-delimited alphas",
                "inputs": {
                    "PropertyAd": "410-A/B/C DANIELS ST, WILLIS, TX  77378"
                },
                "expected": {
                    "number": "410",
                    "street": "DANIELS ST",
                    "unit": "-A/B/C",
                    "city": "WILLIS",
                    "region": "TX",
                    "postcode": "77378"
                }
            },
            {
                "description": "number has alpha and unhyphenated",
                "inputs": {
                    "PropertyAd": "24103A SPENCER BLVD, MAGNOLIA, TX  77355"
                },
                "expected": {
                    "number": "24103",
                    "street": "SPENCER BLVD",
                    "unit": "A",
                    "city": "MAGNOLIA",
                    "region": "TX",
                    "postcode": "77355"
                }
            },
            {
                "description": "number-less address",
                "inputs": {
                    "PropertyAd": "KINGS MANOR DR, HUMBLE, TX  77339"
                },
                "expected": {
                    "number": "",
                    "street": "KINGS MANOR DR",
                    "unit": "",
                    "city": "HUMBLE",
                    "region": "TX",
                    "postcode": "77339"
                }
            },
            {
                "description": "street starts with number",
                "inputs": {
                    "PropertyAd": "1ST ST, SPLENDORA, TX  77372"
                },
                "expected": {
                    "number": "",
                    "street": "1ST ST",
                    "unit": "",
                    "city": "SPLENDORA",
                    "region": "TX",
                    "postcode": "77372"
                }
            },
            {
                "description": "street starts with number",
                "inputs": {
                    "PropertyAd": "67  1/2 APRIL WIND DR S, MONTGOMERY, TX  77356"
                },
                "expected": {
                    "number": "67  1/2",
                    "street": "APRIL WIND DR S",
                    "unit": "",
                    "city": "MONTGOMERY",
                    "region": "TX",
                    "postcode": "77356"
                }
            }
        ]
    }
}
