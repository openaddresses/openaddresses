                "name": "city",
                "data": "https://gisproxy.culvercity.org/arcgis/rest/services/PropInfoData/MapServer/50",
                "protocol": "ESRI",
                "conform": {
                    "format": "geojson",
                    "number": [
                        "ADR_NUM_BS",
                        "ADR_NUM_SX"
                    ],
                    "street": [
                        "ADR_ST_NM",
                        "ADR_ST_TP"
                    ],
                    "unit": [
                        "ADR_UN_TP",
                        "ADR_UNIT"
                    ],
                    "city": {
                        "function": "regexp",
                        "field": "CTY_ST_ZIP",
                        "pattern": "^.+?(?:\\s+CA\\s+\\d{5})$",
                        "replace": "$1"
                    },
                    "region": {
                        "function": "regexp",
                        "field": "CTY_ST_ZIP",
                        "pattern": "\\bCA\\b"
                    },
                    "postcode": {
                        "function": "regexp",
                        "field": "CTY_ST_ZIP",
                        "pattern": "^(?:.+?)\\d{5})$"
                    }
                }
            }
        ],
        "parcels": [
