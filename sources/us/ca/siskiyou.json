{
    "coverage": {
        "US Census": {"geoid": "06093", "name": "Siskiyou County", "state": "California"},
        "country": "us",
        "state": "ca",
        "county": "Siskiyou"
    },
    "note": "Obtained via FOIA request - ingalls - 2017/11, updated by justin - 2018/02",
    "data": "https://www.dropbox.com/s/mt6jp62lecrcfdh/sisk.csv?dl=1",
    "protocol": "http",
    "conform": {
        "format": "csv",
        "lat": "POINT_Y",
        "lon": "POINT_X",
        "number": {
            "function": "prefixed_number",
            "field": "ADDR_1"
        },
        "street": {
            "function": "postfixed_street",
            "field": "ADDR_1",
            "may_contain_units": true
        },
        "unit": {
            "function": "postfixed_unit",
            "field": "ADDR_1"
        },
        "city": {
            "function": "regexp",
            "field": "ADDR_2",
            "pattern": "(.*) CA .*$",
            "replace": "$1"
        },
        "postcode": {
            "function": "regexp",
            "field": "ADDR_2",
            "pattern": ".*([0-9]{5})$",
            "replace": "$1"
        }
    }
}
