{
  "coverage": {
    "country": "us",
    "state": "nd",
    "city": "Dickinson"
  },
  "type": "ESRI",
  "data": "http://maps5.gisworkshop.com/ArcGIS/rest/services/CityofDickinsonElectric/MapServer/4",
  "conform": {
    "type": "geojson",
    "number": {
      "function": "regexp",
      "field": "PROPADDRESS",
      "pattern": "^([0-9]+)"
    },
    "street": {
      "function": "regexp",
      "field": "PROPADDRESS",
      "pattern": "^(?:[0-9]+ )(.*)",
      "replace": "$1"
    }
  }
}