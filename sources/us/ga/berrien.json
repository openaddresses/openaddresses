{
    "coverage": {
        "US Census": {"geoid": "13019", "name": "Berrien County", "state": "Georgia"},
        "country": "us",
        "state": "ga",
        "county": "Berrien"
    },
    "data": "http://www.sgrcmaps.com/arcgis/rest/services/Berrien/AddressLocations/MapServer/0",
    "note": "street is occasionally an intersection, eg - N FIVE ASH ST AT N ELM ST, the regex uses everything up to 'AT' or '@'.",
    "type": "ESRI",
    "conform": {
        "type": "geojson",
        "number": "AddNum",
        "street": {
            "function": "regexp",
            "field": "StreetName",
            "pattern": "^(.+?)(?:\\s+(?:AT|@)\\s+.+)?$"
        },
        "city": "City",
        "postcode": {
            "function": "regexp",
            "field": "ZipCode",
            "pattern": "^(\\d+)(?:.*)$"
        }
    }
}
