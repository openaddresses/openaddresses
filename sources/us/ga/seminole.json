{
    "coverage": {
        "country": "us",
        "state": "ga",
        "county": "Seminole"
    },
    "attribution": "Pierce County",
    "data": "http://data.openaddresses.io/cache/us-ga-seminole_county.csv",
    "type": "http",
    "conform": {
        "type": "csv",
        "lat": "LAT",
        "lon": "LNG",
        "number": {
            "function": "regexp",
            "field": "STR"
            "pattern": "^([0-9]+)( .*)",
            "replace": "$1"
        },
        "street": {
            "function": "regexp",
            "field": "STR"
            "pattern": "^(?:[0-9]+ )(.*)",
            "replace": "$1"
        },
        "region": "REGION"
    }
}
