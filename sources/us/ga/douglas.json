{
    "coverage": {
        "US Census": {
            "geoid": "13097",
            "name": "Douglas County",
            "state": "Georgia"
        },
        "country": "us",
        "state": "ga",
        "county": "Douglas"
    },
    "data": "http://www.gis.dcga.us/arcgis/rest/services/MapLayers/MapServer/13",
    "type": "ESRI",
    "conform": {
        "type": "geojson",
        "number": {
            "function": "prefixed_number",
            "field": "PropertyAddress"
        },
        "street": {
            "function": "regexp",
            "field": "PropertyAddress",
            "pattern": "^(?:[0-9]+)(.+?)(?:\\s+(?:UNIT|APT|BLDG|STE)\\s+.+)?$"
        },
        "unit": {
            "function": "regexp",
            "field": "PropertyAddress",
            "pattern": " ((?:UNIT|APT|BLDG|STE)\\s+.+)$"
        }
    },
    "test": {
        "enabled": true,
        "description": "these are the tests that exercise extracting embedded units",
        "acceptance-tests": [
            {
                "description": "address without unit",
                "inputs": {
                    "PropertyAddress": "7392 BREEZE ST"
                },
                "expected": {
                    "number": "7392",
                    "street": "BREEZE ST",
                    "unit": ""
                }
            },
            {
                "description": "address with APT unit",
                "inputs": {
                    "PropertyAddress": "13 CRESTVIEW DR APT A"
                },
                "expected": {
                    "number": "13",
                    "street": "CRESTVIEW DR",
                    "unit": "APT A"
                }
            }
        ]
    }
}
