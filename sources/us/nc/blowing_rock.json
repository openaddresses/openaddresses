{
    "coverage": {
        "country": "us",
        "state": "nc",
        "place": "Blowing Rock"
    },
    "data": "http://arcgis.webgis.net/arcgis/rest/services/NC/BlowingRock/MapServer/6",
    "type": "ESRI",
    "conform": {
        "street": {
            "function": "regexp",
            "field": "PropAddress",
            "pattern": "^(?:[0-9]+ )(.*)",
            "replace": "$1"
        },
        "number": {
            "function": "regexp",
            "field": "PropAddress",
            "pattern": "^([0-9]+)"
        },
        "type": "geojson"
    }
}
