            "name": "Carlton County",
            "state": "Minnesota"
        },
        "country": "us",
        "state": "mn",
        "county": "Carlton"
    },
    "data": "http://www.carltoncountygis.com/arcgis/rest/services/LINK/Public_LINK_MapLayers/MapServer/7",
    "type": "ESRI",
    "conform": {
        "type": "geojson",
        "number": {
            "function": "regexp",
            "field": "PHYSADDR",
            "pattern": "^([0-9]+)"
        },
        "street": {
            "function": "regexp",
            "field": "PHYSADDR",
            "pattern": "^(?:[0-9]+) (.*)",
            "replace": "$1"
        },
        "city": {
            "function": "regexp",
            "field": "PHYSCITY",
            "pattern": "^(.*) MN$",
            "replace": "$1"
        },
        "region": {
            "function": "regexp",
            "field": "PHYSCITY",
            "pattern": "^.* (MN)$",
            "replace": "$1"
        },
        "postcode": "PHYSZIP"
    }
}
