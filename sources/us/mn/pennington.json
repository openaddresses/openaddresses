{
    "coverage": {
        "US Census": {
            "geoid": "27113",
            "name": "Pennington County",
            "state": "Minnesota"
        },
        "country": "us",
        "state": "mn",
        "county": "Pennington"
    },
    "schema": 2,
    "layers": {
        "addresses": [
            {
                "name": "county",
                "data": "http://gismap.co.pennington.mn.us/arcgis/rest/services/Pennington/Pennington_Internal/MapServer/29",
                "protocol": "ESRI",
                "conform": {
                    "format": "geojson",
                    "number": {
                        "function": "regexp",
                        "field": "PROPERTY_ADDRESS",
                        "pattern": "^([0-9]+)"
                    },
                    "street": {
                        "function": "regexp",
                        "field": "PROPERTY_ADDRESS",
                        "pattern": "^(?:[0-9]+) (.*)",
                        "replace": "$1"
                    },
                    "city": {
                        "function": "regexp",
                        "field": "PROPERTY_CITY",
                        "pattern": "^([^,]+)"
                    },
                    "postcode": "PROPERTY_ZIP"
                }
            }
        ]
    }
}
